{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "cSpell.language": "en-GB",
  "debug.showVariableTypes": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.fontFamily": "'Comic Code Ligatures', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 1200,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmPasteNative": false,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.enableSmartCommit": true,
  "indentRainbow.colors": [
    "rgba(98, 71, 170,0.15)",
    "rgba(114, 81, 181,0.15)",
    "rgba(129, 90, 192, 0.15)",
    "rgba(145, 99, 203,0.15)",
    "rgba(160, 108, 213,0.15)",
    "rgba(177, 133, 219,0.15)",
    "rgba(193, 158, 224,0.15)",
    "rgba(193, 158, 224,0.1)",
    "rgba(193, 158, 224,0.05)"
  ],
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "material-icon-theme.folders.color": "#e0af68",
  "material-icon-theme.folders.theme": "specific",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.stickyScroll.enabled": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "compact",
  "window.title": "${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none"
}