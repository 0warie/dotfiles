{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.language": "en-GB,en-US",
  "cSpell.userWords": ["freezed", "WCAG"],
  "chaliceIcons.showArrows": true,
  "dart.debugExternalPackageLibraries": true,
  "dart.openDevTools": "flutter",
  "dart.showTodos": false,
  "debug.showVariableTypes": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.fontFamily": "'Comic Code Ligatures', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 1200,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmPasteNative": false,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "indentRainbow.colors": [
    "rgba(187,154,247,0.10)", // bb9af7 - Terminal Magenta
    "rgba(170,158,245,0.10)", // Transition
    "rgba(155,162,243,0.10)", // Transition
    "rgba(122,162,247,0.10)", // 7aa2f7 - Terminal Blue
    "rgba(107,175,249,0.10)", // Transition
    "rgba(92,188,251,0.10)", // Transition
    "rgba(125,207,255,0.10)", // 7dcfff - Terminal Cyan
    "rgba(92,188,251,0.10)", // Transition back
    "rgba(107,175,249,0.10)", // Transition
    "rgba(122,162,247,0.10)", // Back through blue
    "rgba(155,162,243,0.10)", // Transition
    "rgba(170,158,245,0.10)" // Back towards magenta
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "material-icon-theme.files.color": "#90a4ae",
  "material-icon-theme.folders.color": "#e0af68",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.saturation": 1,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.stickyScroll.enabled": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "compact",
  "window.title": "${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.experimental.enableNewProfilesUI": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "application.shellEnvironmentResolutionTimeout": 20,
  "files.autoSave": "onFocusChange"

}
