{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "cSpell.language": "en-GB",
  "dart.flutterSdkPath": "/home/luke/Development/flutter",
  "debug.showVariableTypes": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.fontFamily": "'Comic Code Ligatures', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 1200,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmPasteNative": false,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.enableSmartCommit": true,
  "indentRainbow.colors": [
    "rgba(100, 190, 255, 0.10)", // Bright sky blue
    "rgba(130, 160, 255, 0.10)", // Azure blue
    "rgba(160, 130, 255, 0.10)", // Blue-purple
    "rgba(190, 100, 255, 0.10)", // Bright purple
    "rgba(220, 70, 255, 0.10)", // Vibrant pink-purple
    "rgba(190, 100, 255, 0.10)", // Bright purple
    "rgba(160, 130, 255, 0.10)", // Blue-purple
    "rgba(130, 160, 255, 0.10)" // Azure blue
  ],
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "material-icon-theme.folders.color": "#e0af68",
  "material-icon-theme.folders.theme": "specific",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.stickyScroll.enabled": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "compact",
  "window.title": "${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none"
}
