{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "application.shellEnvironmentResolutionTimeout": 20,
  "cSpell.language": "en-GB,en-US",
  "cSpell.userWords": ["freezed", "WCAG"],
  "dart.openDevTools": "flutter",
  "dart.showTodos": false,
  "debug.showVariableTypes": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.fontFamily": "'Comic Code Ligatures', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 1200,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmPasteNative": false,
  "files.autoSave": "onFocusChange",
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "indentRainbow.colors": [
    "rgba(100,190,255,0.12)",
    "rgba(120,170,255,0.12)",
    "rgba(140,150,255,0.12)",
    "rgba(160,130,255,0.12)",
    "rgba(180,110,255,0.12)",
    "rgba(200,90,255,0.12)",
    "rgba(220,70,255,0.12)",
    "rgba(220,70,255,0.12)",
    "rgba(200,90,255,0.12)",
    "rgba(180,110,255,0.12)",
    "rgba(160,130,255,0.12)",
    "rgba(100,190,255,0.12)"
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "material-icon-theme.files.color": "#90a4ae",
  "material-icon-theme.folders.color": "#e0af68",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.saturation": 1,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.stickyScroll.enabled": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "compact",
  "window.title": "${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.experimental.enableNewProfilesUI": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none"
}
